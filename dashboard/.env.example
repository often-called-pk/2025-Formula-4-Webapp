# dashboard/.env.example
# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Application Configuration
VITE_APP_NAME=Formula 4 Race Analytics
VITE_APP_VERSION=2.0.0
VITE_APP_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# File Upload Configuration
VITE_MAX_FILE_SIZE=50000000
VITE_ALLOWED_FILE_TYPES=.csv,.json,.ldx,.ld,.xls,.xlsx

# Real-time Configuration
VITE_REALTIME_ENABLED=true
VITE_REALTIME_HEARTBEAT_INTERVAL=30000

# Analytics Configuration
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_TRACKING_ID=

# Debug Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info

# Authentication Configuration
VITE_AUTH_REDIRECT_URL=http://localhost:5173/auth/callback
VITE_AUTH_PASSWORD_RESET_URL=http://localhost:5173/reset-password

# Storage Configuration
VITE_STORAGE_MAX_FILE_SIZE=52428800
VITE_STORAGE_ALLOWED_MIME_TYPES=text/csv,application/json,image/png,image/jpeg

# Feature Flags
VITE_FEATURE_LIVE_TELEMETRY=true
VITE_FEATURE_DRIVER_COMPARISON=true
VITE_FEATURE_REAL_TIME_ANALYSIS=true
VITE_FEATURE_ADVANCED_ANALYTICS=false

# Map Configuration
VITE_MAP_DEFAULT_ZOOM=15
VITE_MAP_MAX_ZOOM=20
VITE_MAP_MIN_ZOOM=10

# Telemetry Configuration
VITE_TELEMETRY_SAMPLE_RATE=100
VITE_TELEMETRY_BUFFER_SIZE=1000
VITE_TELEMETRY_MAX_POINTS=100000

# UI Configuration
VITE_THEME_DEFAULT=dark
VITE_CHART_ANIMATION_DURATION=300
VITE_TABLE_PAGE_SIZE=50